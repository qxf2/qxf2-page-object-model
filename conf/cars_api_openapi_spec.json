  "openapi": "3.0.3",
  "components": {
    "schemas": {
      "HTTPError": {
        "properties": {
          "detail": {
            "type": "object"
          },
          "message": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "ValidationError": {
        "properties": {
          "detail": {
            "type": "object",
            "properties": {
              "<location>": {
                "type": "object",
                "properties": {
                  "<field_name>": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "message": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "CarListOutput": {
        "type": "object",
        "properties": {
          "cars_list": {
            "type": "array",
            "items": {
              "type": "object"
            }
          },
          "successful": {
            "type": "boolean"
          }
        },
        "required": [
          "successful"
        ]
      },
     "CarFullDetails": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "brand": {
            "type": "string"
          },
          "price_range": {
            "type": "string"
          },
          "car_type": {
            "type": "string"
          }
        },
        "required": [
          "brand",
          "name"
        ]
      },
      "CarDetailsOutput": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "car": {
            "type": "object"
          },
          "successful": {
            "type": "boolean"
          }
        },
        "required": [
          "successful"
        ]
      },
     "RemoveCarOutput": {
        "type": "object",
        "properties": {
          "car": {
            "type": "object"
          },
          "successful": {
            "type": "boolean"
          }
        },
        "required": [
          "car",
          "successful"
        ]
      },
      "Status": {
        "type": "object",
        "properties": {
          "successful": {
            "type": "boolean"
          }
        },
        "required": [
          "successful"
        ]
      },
      "CarFilterOutput": {
        "type": "object",
        "properties": {
          "cars": {
            "type": "array",
            "items": {
              "type": "object"
            }
          }
        },
        "required": [
          "cars"
        ]
      }
    },
    "securitySchemes": {
      "BasicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    }
